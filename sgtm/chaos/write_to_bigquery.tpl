const addEventCallback = require('addEventCallback');
const BigQuery = require('BigQuery');
const getTimestampMillis = require('getTimestampMillis');
const JSON = require('JSON');
const log = require('logToConsole');
const Math = require('Math');

// function for creating the date
function createDateString(currentTimestamp) {
  const timestampDays = Math.floor(currentTimestamp / 60 / 60 / 24 / 1000);
  const days = timestampDays + 719468;
  const era = Math.floor((days >= 0 ? days : days - 146096) / 146097);
  const doe = days - era * 146097;
  const yoe = Math.floor((doe + (-Math.floor(doe / 1460)) + Math.floor(doe / 36524) - Math.floor(doe / 146096)) / 365);
  const y = yoe + era * 400;
  const doy = doe - (365 * yoe + Math.floor(yoe / 4) - Math.floor(yoe / 100));
  const mp = Math.floor((5 * doy + 2) / 153);
  const d = 1 + doy - Math.floor(((153 * mp) + 2) / 5);
  const m = mp < 10 ? mp + 3 : mp - 9;
  let timeLeft = (currentTimestamp - (timestampDays * 60 * 60 * 24 * 1000)) / 1000;
  const hours = Math.floor(timeLeft / 60 / 60);
  timeLeft = timeLeft - hours * 60 * 60;
  const minutes = Math.floor(timeLeft / 60);
  timeLeft = timeLeft - minutes * 60;
  const seconds = Math.floor(timeLeft);
  timeLeft = timeLeft - seconds;
  
  function addZeroIfNeeded(number) {
    if (number < 10) {
      return '0' + number;
    } else {
      return '' + number;
    }
  }
  const dateArray = {
    year: y + (m <= 2),
    month: addZeroIfNeeded(m),
    day: addZeroIfNeeded(d),
  };
  return dateArray.year + dateArray.month + dateArray.day;
}

// creating timestamp and date variables
const currentTimestamp = getTimestampMillis();
const date = createDateString(currentTimestamp);
const eventData = {};

// Adding timestamp into eventData object if it is checked
if(data.timestamp){
  eventData[data.timestampFieldName] = currentTimestamp ? currentTimestamp : null;
}

// Adding date into eventData object if it is checked
if(data.date){
  eventData[data.dateFieldName] = date ? date : null;

}

// Adding add attributes into eventData object
if(data.attributesList) {
  data.attributesList.forEach(param => {
    eventData[param.field] = param.value ? param.value : null;
  });
}


// Sending data to BigQuery
addEventCallback(() => {
  BigQuery.insert({
    projectId: data.gcpProjectId,
    datasetId: data.datasetId,
    tableId: data.tableId,
  }, [eventData], {}, () => {
    log('BigQuery Success: ', [eventData]);
  }, (errors) => {
    log('BigQuery Failure: ', JSON.stringify(errors));
  });
});


data.gtmOnSuccess();


